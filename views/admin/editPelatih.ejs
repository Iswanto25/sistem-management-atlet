<form id="add-row-form" action="/admin/Update/pelatih" method="post">
    <div id="editpelatih" class="modal fade">
        <div class="modal-dialog modal-dialog-centered modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Edit Data pelatih</h5>
                </div>
                <div class="modal-body">
                    <form action="">
                        <div class="row">
                            <div class="col form-group">
                                <label for="nik" class="form-label">NIK</label>
                                <input id="nik" type="text" name="nik" class="form-control nik">
                            </div>
                            <div class="col form-group">
                                <label for="nama" class="form-label">Nama</label>
                                <input id="nik" type="text" name="nama" class="form-control nama">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col form-group">
                                <label for="tempat_lahir" class="form-label">Tempat Lahir</label>
                                <input id="tempat_lahir" type="text" name="tempat_lahir"
                                    class="tempat_lahir form-control">
                            </div>
                            <div class="col form-group">
                                <label for="tgl_lahir" class="form-label">Tanggal Lahir</label>
                                <input id="tgl_lahir" type="date" name="tgl_lahir" class="form-control tgl_lahir">
                            </div>
                            <div class="col form-group">
                                <label for="jns_kelamin" class="form-label">Jenis Kelamin</label>
                                <select type="text" name="jns_kelamin" class="custom-select mr-sm-2 jns_kelamin"
                                    id="model inlineFormCustomSelectPref">
                                    <option selected>Jenis Kelamin</option>
                                    <option value="Laki-laki">Laki-laki</option>
                                    <option value="Perempuan">Perempuan</option>
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col form-group">
                                <label for="alamat" class="form-label">Alamat</label>
                                <textarea class="form-control alamat" name="alamat" id="alamat" rows="3"></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col form-group">
                                <label for="kecamatan" class="form-label">Kecamatan</label>
                                <select type="text" name="kecamatan" class="custom-select mr-sm-2 kecamatan"
                                    id="model inlineFormCustomSelectPref">
                                    <option selected>Kecamatan</option>
                                    <% cabor.forEach(function(cabor){ %>
                                        <option value="<%= cabor.kecamatan %>">
                                            <%= cabor.kecamatan %>
                                        </option>
                                        <%});%>
                                </select>
                            </div>
                            <div class="col form-group">
                                <label for="cabor" class="form-label">Cabor</label>
                                <select type="text" name="cabor" class="cabor custom-select mr-sm-2">
                                    <% cabor.forEach(function(cabor){ %>
                                    <option selected>Cabang Olahraga</option>
                                        <option value="<%= cabor.cabor %>">
                                            <%= cabor.cabor %>
                                        </option>
                                        <%});%>
                                </select>
                            </div>
                            <div class="col form-group">
                                <label for="jabatan" class="form-label">Jabatan</label>
                                <input type="text" class="form-control jabatan" name="jabatan">
                            </div>
                            <div class="col form-group">
                                <label for="tgl_gabung" class="form-label tgl_gabung">Tanggal Gabung</label>
                                <input id="tgl_gabung" type="date" name="tgl_gabung" class="form-control tgl_gabung">
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <input type="submit" name="id_pelatih" class="id_pelatih">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary id_pelatih" name="id_pelatih">Update</button>
                </div>
            </div>
        </div>
    </div>
</form>